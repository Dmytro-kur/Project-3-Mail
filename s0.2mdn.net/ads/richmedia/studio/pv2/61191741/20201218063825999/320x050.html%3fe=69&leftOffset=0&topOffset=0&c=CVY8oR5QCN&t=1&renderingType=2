/**
 *  Shrinks timeline time to 15s
 *
 * @param {Object} timeline timeline
 * @param {Number} gdn Check if Timeline has to fit in 15s
 * @param {Boolean} noRepeat Should it repeat
 * @param {String} adId Format Id
 */

var setMaxTimeRepeat = function setMaxTimeRepeat(timeline, gdn, noRepeat, adId) {
  var duration = timeline.duration();
  var repeatDelay = 2,
      maxTime = 15;

  if (noRepeat) {
    Object(_animations_cta1__WEBPACK_IMPORTED_MODULE_0__["cta1"])(timeline, adId);
    timeline.repeat(0);
    timeline.play();
    return;
  }

  if (gdn !== 1) {
    Object(_animations_cta1__WEBPACK_IMPORTED_MODULE_0__["cta1"])(timeline, adId);
    Object(_animations_outro__WEBPACK_IMPORTED_MODULE_1__["outro"])(timeline, adId);
    timeline.repeat(-1);
    timeline.play();
    return;
  }

  if (duration > maxTime) {
    Object(_animations_cta1__WEBPACK_IMPORTED_MODULE_0__["cta1"])(timeline, adId);
    var timeScaleValue = timeline.duration() / maxTime;
    timeline.timeScale(timeScaleValue);
  } else {
    var rawLoops = Math.floor(maxTime / duration);
    var loops = rawLoops;

    for (loops = rawLoops; (loops - 1) * repeatDelay + loops * duration >= maxTime;) {
      loops = loops - 1;
    }

    timeline.repeatDelay(0);
    timeline.repeat(loops - 1);
    Object(_animations_cta1__WEBPACK_IMPORTED_MODULE_0__["cta1"])(timeline, adId);
    var runs = 1;
    /**
     * Stop GSAP Animation
     *
     */

    var stopAnimation = function stopAnimation() {
      runs++;

      if (runs >= loops) {
        timeline.addPause('pause');
      }
    };

    if (loops > 1) {
      Object(_animations_outro__WEBPACK_IMPORTED_MODULE_1__["outro"])(timeline, adId);
      timeline.eventCallback('onRepeat', stopAnimation);
    }
  }

  timeline.play();
};

