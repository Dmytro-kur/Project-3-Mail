/**
 * getNumberOfTextLines
 * get number of lines of html element containing text
 * @param {object} el - html element containing the text
 * @return {number} number of lines
 */
var getNumberOfTextLines = function getNumberOfTextLines(el) {
  var elHeight, elLineHeight, numLines, elPadding;
  elHeight = parseFloat(el.offsetHeight);
  elLineHeight = parseFloat(window.getComputedStyle(el, null).getPropertyValue('line-height'));

  if (isNaN(elLineHeight)) {
    elLineHeight = parseFloat(window.getComputedStyle(el, null).getPropertyValue('font-size')) * 1.2;
  }

  elPadding = parseFloat(window.getComputedStyle(el).paddingTop) * 2;

  if (isNaN(elPadding)) {
    elPadding = 0;
  }

  numLines = Math.ceil((elHeight - elPadding) / elLineHeight);
  return numLines;
};
/**
 * downsizeText
 * decrease fontsize of multiline text until text fits
 * @param  {string} elementSelector element class containing text to size
 * @param  {number} maxLines max number of text lines allowed
 * @param  {number} minFontSize minimum font size allowed
 * @param  {boolean} downsizeLineHeight flag if lineheight should also decrease
 */


var downsizeText = function downsizeText(elementSelector, maxLines, minFontSize, downsizeLineHeight) {
  var fontSize, lineHeight, numLines;
  var el = document.querySelector(elementSelector);

  if (el === null) {
    return;
  }

  fontSize = parseFloat(window.getComputedStyle(el, null).getPropertyValue('font-size'));
  lineHeight = parseFloat(window.getComputedStyle(el, null).getPropertyValue('line-height'));
  numLines = getNumberOfTextLines(el);

  while (numLines > maxLines) {
    fontSize = fontSize - 0.5;
    lineHeight = lineHeight - 0.5;

    if (fontSize < minFontSize) {
      break;
    } else {
      el.style.fontSize = fontSize + 'px';

      if (downsizeLineHeight === true) {
        el.style.lineHeight = lineHeight + 'px';
      }
    }

    numLines = getNumberOfTextLines(el);
  }

  el.style.maxWidth = 'none';
};
/**
 * calculateTextSize
 * decrease fontsize of multiline text until text fits
 * @param  {string} elementSelector element class containing text to size
 * @param  {string} elementSelector2 element class containing text to size
 * @param  {number} maxLines max number of text lines allowed
 * @param  {number} minFontSize minimum font size allowed
 * @param  {boolean} downsizeLineHeight flag if lineheight should also decrease
 * @param  {boolean} centerElements center elements
 */

var calculateTextSize = function calculateTextSize(elementSelector, elementSelector2, maxLines, minFontSize, downsizeLineHeight) {
  var fontSize, lineHeight, numLines;
  var el = document.querySelector(elementSelector),
      childEl = document.querySelector(elementSelector2);

  if (el === null || childEl === null) {
    return;
  }

  fontSize = parseFloat(window.getComputedStyle(childEl, null).getPropertyValue('font-size'));
  lineHeight = parseFloat(window.getComputedStyle(childEl, null).getPropertyValue('line-height'));
  numLines = getNumberOfTextLines(childEl);

  while (numLines > maxLines) {
    fontSize = fontSize - 0.5;
    lineHeight = fontSize * 1.3;

    if (fontSize < minFontSize) {
      break;
    } else {
      childEl.style.fontSize = fontSize + 'px';

      if (downsizeLineHeight === true) {
        childEl.style.lineHeight = lineHeight + 'px';
      }
    }

    numLines = getNumberOfTextLines(childEl);
  } // childEl.style.display = 'inline-block';


  while (el.offsetWidth < childEl.scrollWidth) {
    fontSize = fontSize - 0.5;
    lineHeight = fontSize * 1.3;
    childEl.style.fontSize = fontSize + 'px';

    if (downsizeLineHeight === true) {
      childEl.style.lineHeight = lineHeight + 'px';
    }

    if (fontSize < 2) {
      break;
    }
  }

  numLines = getNumberOfTextLines(childEl); // if (numLines === 1) {
  //     lineHeight = fontSize * 1;
  //     childEl.style.lineHeight = lineHeight + 'px';
  // }
};
