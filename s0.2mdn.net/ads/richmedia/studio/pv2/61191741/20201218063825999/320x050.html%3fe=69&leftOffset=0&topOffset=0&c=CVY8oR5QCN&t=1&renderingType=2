/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "clock", function() { return clock; });
/**
 * Get Remaining Time
 *
 * @param {String} endtime Endtime String
 * @returns {Object} Endtime Object {Total, Tage, Std, Min, Sek}
 */
var getTimeRemaining = function getTimeRemaining(endtime) {
  var t = Date.parse(endtime) - Date.parse(new Date());
  return {
    Total: t,
    Tage: Math.floor(t / (1000 * 3600 * 24)),
    Std: Math.floor(t / (1000 * 3600) % 24),
    Min: Math.floor(t / 1000 / 60 % 60),
    Sek: Math.floor(t / 1000 % 60)
  };
};
/**
 * CountdownTracker
 * calculate and set gdn stop time
 * @param {string} label - label string
 * @param {string} value - value string
 */


function CountdownTracker(label, value) {
  var top,
      bottom,
      back,
      backBottom,
      el = document.createElement('div');
  el.className = 'flip-clock__piece';
  el.innerHTML = "\n        <div class=\"flip-clock__card card\">\n            <div class=\"card__top\"></div>\n            <div class=\"card__bottom\" data-value=\"88\"></div>\n            <div class=\"card__back\" data-value=\"88\">\n                <div class=\"card__bottom\" data-value=\"88\"></div>\n            </div>\n        </div>\n        <div class=\"flip-clock__slot\">".concat(label, "</div>");
  this.el = el;
  top = el.querySelector('.card__top');
  bottom = el.querySelector('.card__bottom');
  back = el.querySelector('.card__back');
  backBottom = el.querySelector('.card__back .card__bottom');

  this.update = function (OrigVal) {
    var val = ('0' + OrigVal).slice(-2);

    if (val !== this.currentValue) {
      if (this.currentValue >= 0) {
        back.setAttribute('data-value', this.currentValue);
        bottom.setAttribute('data-value', this.currentValue);
      }

      this.currentValue = val;
      top.innerText = this.currentValue;
      backBottom.setAttribute('data-value', this.currentValue);
      this.el.classList.remove('flip'); // eslint-disable-next-line no-void

      void this.el.offsetWidth;
      this.el.classList.add('flip');
    }
  };

  this.update(value);
}
/**
 * Countdown clock
 *
 * @param {String} RawValue the content object
 */


var clock = function clock(RawValue) {
  var i = 0,
      t,
      key,
      updateFn = getTimeRemaining,
      // timeinterval,
  trackers = {};
  var countdown = new Date(RawValue);
  var elements = document.querySelectorAll('div.flip-clock');
  Array.prototype.forEach.call(elements, function (el) {
    t = updateFn(countdown);

    for (key in t) {
      if (key === 'Total') {
        continue;
      }

      trackers[key] = new CountdownTracker(key, t[key]);
      el.appendChild(trackers[key].el);
    }
  });
  /**
   * updateClock
   * calculate and set gdn stop time
   */

  function updateClock() {
    var t;
    i++;

    if (i % 10) {
      return;
    }

    t = updateFn(countdown);

    if (t.Total < 0) {
      for (key in trackers) {
        trackers[key].update(0);
      }

      return;
    }

    for (key in trackers) {
      trackers[key].update(t[key]);
    }
  }

  setInterval(updateClock, 100);
};

