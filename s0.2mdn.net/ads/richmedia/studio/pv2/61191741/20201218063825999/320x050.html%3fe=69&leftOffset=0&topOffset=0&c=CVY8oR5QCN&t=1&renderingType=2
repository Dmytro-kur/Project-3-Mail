/**
 * getPlaceholders - extracts names of placeholder from string and puts into array
 * @param  {string} str str from which to extract placeholders
 * @returns {array} array with names of placeholders
 */

var getPlaceholders = function getPlaceholders(str) {
  var regex = /{(\w+)}/g;
  var arr = [],
      match; // eslint-disable-next-line no-cond-assign

  while (match = regex.exec(str)) {
    arr.push(match[1]);
  }

  return arr;
};
/**
 * getUrlParams - extracts params from url and returns object with name/value pairs
 * @param  {string} url url from which to extract params
 * @returns {object} object with name/value pairs
 */


var getUrlParams = function getUrlParams(url) {
  var i, pair, val;
  var query = url,
      paramsObj = {},
      paramsArr = query.split(/[?&]+/);

  if (paramsArr.length <= 0) {
    return paramsObj;
  }

  for (i = 0; i < paramsArr.length; i++) {
    pair = paramsArr[i].split('=');
    val = pair[1]; //decodeURIComponent(pair[1]);

    if (val !== '"' && pair[0] !== '"') {
      paramsObj[pair[0]] = val;
    }
  }

  return paramsObj;
};
/**
 * Replace Placeholders in Object
 *
 * @param {object} dynamicContent the content object
 * @returns {object} content object with replaced placeholders
 */


var replacePlaceholders = function replacePlaceholders(dynamicContent) {
  var content = dynamicContent;

  for (var cont in content) {
    if (typeof content[cont] === 'string') {
      var matches = getPlaceholders(content[cont]);

      if (matches) {
        var _iterator = _createForOfIteratorHelper(matches),
            _step;

        try {
          for (_iterator.s(); !(_step = _iterator.n()).done;) {
            var match = _step.value;
            var string = content[cont],
                placeholder = "{".concat(match, "}");
            string = string.replace(placeholder, content[match]);
            content[cont] = string;
          }
        } catch (err) {
          _iterator.e(err);
        } finally {
          _iterator.f();
        }
      }
    }
  }

  return content;
};
/**
 * Set TrackingPixel
 *
 * @param {string} Url the tracking Pixel Url
 * @param {string} ProductLink the ProductLink with Url Params
 */

var setPvPixel = function setPvPixel(Url, ProductLink) {
  var content = Url,
      img;

  if (content === '') {
    return;
  }

  var matches = getPlaceholders(content),
      UrlParams = getUrlParams(ProductLink);

  if (matches) {
    var _iterator2 = _createForOfIteratorHelper(matches),
        _step2;

    try {
      for (_iterator2.s(); !(_step2 = _iterator2.n()).done;) {
        var match = _step2.value;
        var placeholder = "{".concat(match, "}");
        content = content.replace(placeholder, UrlParams[match]);
      }
    } catch (err) {
      _iterator2.e(err);
    } finally {
      _iterator2.f();
    }
  }

  img = document.createElement('img');
  img.setAttribute('width', '1');
  img.setAttribute('height', '1');
  img.setAttribute('alt', '');
  img.setAttribute('border', '0');
  img.setAttribute('style', 'display:none');
  img.setAttribute('src', content);
  document.body.appendChild(img);
};
/**
 * Replace Headlines with default if empty
 *
 * @param {object} dynamicContent the content object
 * @returns {object} headline object
 */

var defaultHedlines = function defaultHedlines(dynamicContent) {
  var content = dynamicContent;
  var headlines = {
    city: '<span class="preCity">Auch In deinem Ort</span><span class="city"></span>'
  };

  if (content.City !== '') {
    headlines.city = "<span class=\"preCity\">Auch in</span><span class=\"city\">".concat(content.City, "</span>");
  }

  return headlines;
};
/**
 * Set Stoerer
 *
 * @param {*} dynamicContent Feed Content
 * @returns {String} stoerer HTML
 */

var setStoerer = function setStoerer(dynamicContent) {
  var content = dynamicContent;
  var stoerer = '<div class="stoerer"></div>',
      style = '';

  if (content.L01_ShowStoerer === 0) {
    return stoerer;
  }

  if (content.L01_StoererPositionXY !== 'default' && content.L01_StoererPositionXY !== '') {
    var position = content.L01_StoererPositionXY.split(',');
    style = "left: ".concat(position[0], "px; top: ").concat(position[1], "px; position:absolute");
  }

  stoerer = "<img class=\"stoerer\" src=\"".concat(content.L01_StoererImageAsset.Url, "\" style=\"").concat(style, "\" />");
  return stoerer;
};
