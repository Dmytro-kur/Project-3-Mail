  return offer;
};
/**
 * Postion Difference between device images
 *
 * @param {string} device1 Feed Content
 * @param {string} device2 Feed Content
 * @returns {Object} positions left, top
 */

var deviceDifference = function deviceDifference(device1, device2) {
  var deviceElement1 = document.querySelector(device1),
      deviceElement2 = document.querySelector(device2);
  var deviceElement1Position = deviceElement1.getBoundingClientRect(),
      deviceElement2Position = deviceElement2.getBoundingClientRect(),
      positions = {};
  positions = {
    left: deviceElement2Position.left - deviceElement1Position.left,
    top: deviceElement2Position.top - deviceElement1Position.top,
    scaleOrg: deviceElement1Position.width / deviceElement2Position.width,
    scale: deviceElement2Position.width / deviceElement1Position.width
  };
  return positions;
};

